        "@babel/preset-env": "^7.22.20",
        "@swc/core": "^1.9.2",
        "@types/jest": "^29.5.14",
        "eslint": "^8.50.0",
        "jest": "^29.7.0",
        "jsdoc": "^4.0.2",
        "prettier": "^3.0.3",
        "ts-jest": "^29.2.5",
        "tsup": "^8.3.5",
        "typescript": "^5.6.3"
    }
