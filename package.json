{
    "name": "@cyberfly-io/client",
    "version": "1.1.9",
    "description": "Official SDK for Cyberfly GraphQL API and pubsub",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "node src/lib/index.js",
        "build": "tsup",
        "dev": "nodemon src/lib/index.js",
        "test": "jest",
        "lint": "eslint src/**/*.js",
        "format": "prettier --write \"src/**/*.js\""
    },
    "keywords": [
        "cyberfly",
        "graphql",
        "sdk",
        "database",
        "blockchain"
    ],
    "author": "Cyberfly Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cyberfly-io/cyberfly.js.git"
    },
    "bugs": {
